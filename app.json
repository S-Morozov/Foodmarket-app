{
  "expo": {
    "name": "foodmarketapp",
    "slug": "foodmarketapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/foodexp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",

    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "newArchEnabled": true,
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "newArchEnabled": true,
        "foregroundImage": "./assets/images/dash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
